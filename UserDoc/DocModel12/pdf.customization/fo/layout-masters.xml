			margin-bottom="120pt"
			margin-left="100pt"
			margin-right="60pt"
			column-count="2"
		/>
		<!--region-name="body"-->
		
		<fo:region-before extent="100pt"
			region-name="header"
			display-align="after"
		/>
		
		<fo:region-after extent="120pt"
			region-name="footer"
			display-align="before"
		/>
		
		
		<fo:region-start extent="60pt"
			region-name="left"
		/>
		
		<fo:region-end extent="60pt"
			region-name="right"
		/>
	</fo:simple-page-master>
	

