<!-- Copyright FUJITSU LIMITED 2017-->
<tns:TechnicalServices xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:tns="oscm.serviceprovisioning/1.9/TechnicalService.xsd" xsi:schemaLocation="oscm.serviceprovisioning/1.9/TechnicalService.xsd ../../oscm-serviceprovisioning/javares/TechnicalServices.xsd">
   
   <tns:TechnicalService 
   		id="OPENSTACK_V5" 
   		build="20140708" 
   		accessType="DIRECT" 
   		allowingOnBehalfActing="false" 
   		baseUrl="" 
   		loginPath="" 
   		onlyOneSubscriptionPerUser="false" 
   		provisioningType="ASYNCHRONOUS" 
   		provisioningUrl="https://HOST:PORT/ProvisioningService/AsynchronousProvisioningProxy?wsdl" 
   		provisioningVersion="1.0">
      
      <AccessInfo locale="en">OpenStack Access Info</AccessInfo>
      <LocalizedDescription locale="en">OpenStack IaaS implementation</LocalizedDescription>
      <LocalizedLicense locale="en"></LocalizedLicense>
      <LocalizedTag locale="en">OpenStack</LocalizedTag>
      
      <ParameterDefinition id="APP_CONTROLLER_ID" valueType="STRING" mandatory="true" configurable="false" default="ess.openstack">
         <LocalizedDescription locale="en">ID of the controller implementation</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="RESOURCE_TYPE" valueType="STRING" mandatory="true" configurable="false" default="OS::Nova::Server">
         <LocalizedDescription locale="en">OS::Nova::Server, OS::Keystone::Project</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="STACK_NAME" valueType="STRING" mandatory="true" configurable="true" default="" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">OpenStack instance name</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="STACK_NAME_PATTERN" valueType="STRING" mandatory="true" configurable="true" default="([A-Za-z][A-Za-z0-9_-]*){1,30}" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Validation pattern for the stack name (regex)</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TEMPLATE_NAME" valueType="ENUMERATION" mandatory="true" configurable="true" default="template.json" modificationType="ONE_TIME">
         <Options>
            <Option id="template.json">
               <LocalizedOption locale="en">template.json</LocalizedOption>
            </Option>
            <Option id="template.yaml">
               <LocalizedOption locale="en">template.yaml</LocalizedOption>
            </Option>
         </Options>
         <LocalizedDescription locale="en">Template files for resource orchestration</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TENANT_ID" valueType="STRING" mandatory="true" configurable="true" default="your.tenant.id" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Tenant ID (project ID)</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="ACCESS_INFO_PATTERN" valueType="STRING" mandatory="true" configurable="true" default="Key pair name: {KP_Out}; Assigned network addresses: {NW_Out};" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Pattern for access information</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_ImageId" valueType="STRING" mandatory="true" configurable="true" default="cedarish" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Image ID</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_flavor" valueType="STRING" mandatory="true" configurable="true" default="m1.small" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Instance type (flavor)</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_KeyName" valueType="STRING" mandatory="true" configurable="true" default="openstackkeypair" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Key name</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_az" valueType="STRING" mandatory="true" configurable="true" default="nova" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">Availability zone</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_VmName" valueType="STRING" mandatory="true" configurable="true" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">VmName</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_AdminPassword" valueType="STRING" mandatory="true" configurable="true" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">AdminPassword</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_PrivateNetworkId" valueType="STRING" mandatory="true" configurable="true" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">PrivateNetworkId</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_SubnetId" valueType="STRING" mandatory="true" configurable="true" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">SubnetId</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_PublicNetworkId" valueType="STRING" mandatory="true" configurable="true" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">PublicNetworkId</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="TP_ARRAY_SecurityGroupName" valueType="STRING" mandatory="true" configurable="true" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">SecurityGroupName</LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="MAIL_FOR_COMPLETION" valueType="STRING" mandatory="false" configurable="true" default="" modificationType="ONE_TIME">
         <LocalizedDescription locale="en">The address to which emails are to be sent that describe manual steps required to
            complete an operation. If you specify this parameter, the service controller interrupts
            the processing of each operation before its completion and waits for a notification
            about the execution of a manual action. Omit this parameter if you do not want to
            interrupt the processing.
         </LocalizedDescription>
      </ParameterDefinition>
      
      <ParameterDefinition id="VMS_NUMBER" valueType="LONG" mandatory="true" configurable="false" default="0">
         <LocalizedDescription locale="en">The number of VMs of IaaS subscriptions</LocalizedDescription>
      </ParameterDefinition>
      
      <Operation id="START_VIRTUAL_SYSTEM" actionURL="https://HOST:PORT/OperationService/AsynchronousOperationProxy?wsdl">
         <LocalizedName locale="en">Start</LocalizedName>
         <LocalizedDescription locale="en">Start all servers</LocalizedDescription>
      </Operation>
      
      <Operation id="STOP_VIRTUAL_SYSTEM" actionURL="https://HOST:PORT/OperationService/AsynchronousOperationProxy?wsdl">
         <LocalizedName locale="en">Stop</LocalizedName>
         <LocalizedDescription locale="en">Stop all servers</LocalizedDescription>
      </Operation>
      
      <Operation id="RESUME_VIRTUAL_SYSTEM" actionURL="https://HOST:PORT/OperationService/AsynchronousOperationProxy?wsdl">
         <LocalizedName locale="en">Resume</LocalizedName>
         <LocalizedDescription locale="en">Resume the instance</LocalizedDescription>
      </Operation>
      
      <Operation id="SUSPEND_VIRTUAL_SYSTEM" actionURL="https://HOST:PORT/OperationService/AsynchronousOperationProxy?wsdl">
         <LocalizedName locale="en">Suspend</LocalizedName>
         <LocalizedDescription locale="en">Suspend the instance</LocalizedDescription>
      </Operation>
      
   </tns:TechnicalService>
</tns:TechnicalServices>
