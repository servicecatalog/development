    <ParameterDefinition configurable="true" id="TP_VmName" mandatory="true" modificationType="ONE_TIME" valueType="STRING">
      <LocalizedDescription locale="en">VmName</LocalizedDescription>
      <LocalizedDescription locale="de"/>
      <LocalizedDescription locale="ja"/>
    </ParameterDefinition>
    <ParameterDefinition configurable="true" id="TP_AdminPassword" mandatory="true" modificationType="ONE_TIME" valueType="STRING">
